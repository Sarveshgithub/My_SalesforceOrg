<aura:component controller="ChartController" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
	 <aura:attribute name="sObj" type="String" default=""/>
	<aura:attribute name="field" type="String" default=""/>
    <aura:attribute name="Charttype" type="String" default=""/>
    <aura:attribute name="picklistvalues" type="String[]"/>
    <ltng:require scripts="{!$Resource.chartJS}"/>
    <aura:handler name="init" value="{!this}" action="{!c.doinit}"/>
    <div class="container">
        <!-- Doughnut Chart -->
        <div class="chart-demo" style="background: white;">
            <h2 class="title">Chart.Js</h2>
            <div>
                <canvas class="myCanvas" id="doughnutChart" width="100" height="100"></canvas>
            </div>
        </div>
    </div>
</aura:component>