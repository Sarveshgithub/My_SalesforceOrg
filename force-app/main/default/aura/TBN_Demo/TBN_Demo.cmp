<aura:component controller="TBN_Demo_fetchaccountdata" implements="flexipage:availableForAllPageTypes,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction" access="global">
    
    <aura:attribute name="objContact" type="Contact"/>
    <aura:handler name="init" value="{!this}" action="{!c.loadContactList}"/>
    
    <table>
        <aura:iteration items="{!v.objContact}" var="item">
            <tr>
                <td>
                    {!item.FirstName}
                </td>
                 <td>
                    {!item.Id}
                </td>
            </tr>
        </aura:iteration>
    </table>
</aura:component>